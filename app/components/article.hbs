{{#each @project as | item |}}
  <article class="{{if @isToggled " purple-shadow"}}">
    <a aria-label={{item.aria-label}} href={{item.href}}
      rel="noreferrer noopener" target="_blank">
      <img height='230' src={{item.src}} alt={{item.alt}}>
      <p>{{item.title}}</p>
    </a>
  </article>
{{/each}}
